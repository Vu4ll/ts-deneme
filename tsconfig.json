{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "strict": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true,
        "outDir": "./dist"
    },
    "include": [
        "index.ts",
        "models/**/*.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}